# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'ZhylfWeibo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

pod "AFNetworking"
pod "SDWebImage"
pod "YYModel"
pod "FMDB"
pod "SVProgressHUD"
pod "pop"

  target 'ZhylfWeiboTests' do
    inherit! :search_paths
    # Pods for testing
pod "AFNetworking"
pod "SDWebImage"
pod "YYModel"
pod "FMDB"
pod "SVProgressHUD"
pod "pop"
  end

  target 'ZhylfWeiboUITests' do
    inherit! :search_paths
    # Pods for testing
pod "AFNetworking"
pod "SDWebImage"
pod "YYModel"
pod "FMDB"
pod "SVProgressHUD"
pod "pop"
  end

end
